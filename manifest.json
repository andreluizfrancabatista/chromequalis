{
  "name": "ChromeQualis",
  "version": "3",
  "description": "Confira o Qualis de um periódico",
  "author": "André Luiz França Batista",
  "icons": {
    "16": "images/16_ico_qualis_icon.png",
    "48": "images/48_ico_qualis_icon.png",
    "128": "images/128_ico_qualis_icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "scripting",
    "tabs"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "manifest_version": 3
}